package com.company;

import java.util.*;
class Quadrant

{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the value of x:");
        System.out.println("Enter the value of y:");

        int x,y;
        x = sc.nextInt();
        y = sc.nextInt();
        if ( x > 0 && y > 0 ) {
            System.out.println("Belongs to first quadrant");

        } else if ( x < 0 && y < 0 ) {
            System.out.println("Belongs to fourth quadrant");
        } else if ( x < 0 && y > 0 ) {
            System.out.println("Belongs to second quadrant");
        } else if ( x > 0 && y < 0 ) {
            System.out.println("Belongs to third quadrant");

        } else {
            System.out.println("Lies in the origin");

        }}}
